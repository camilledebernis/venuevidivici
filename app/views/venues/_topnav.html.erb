<div class="row justify-content-end align-items-center">
  <div class="a mx-2">
    <%= link_to 'rent your vvvenue', new_venue_path, method: :get %> <!--  link here needs to be changed -->
  </div>

  <% if current_user %>
    <div class="mx-2">
      <%= cl_image_tag("danielle-the-bartender_uc7vck", :cloud_name => 'danielcastro', width: 50, height: 50,
                        crop: :thumb, gravity: :face, class: "rounded-circle") %>
    </div>
    <div class="a mx-2">
      <button class="btn btn-light">
        <%= link_to 'sign out', destroy_user_session_path, method: :delete %>
      </button>
    </div>
  <% else %>
    <div class="a mx-2">
      <button class="btn btn-primary">
        <%= link_to 'sign in', new_user_session_path, method: :get, class: "no-style" %>
      </button>
    </div>
  <% end %>
</div>
